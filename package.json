    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "webpack": "^4.28.4",
